{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/forms": [
      "default",
      {
        "label": "off",
        "select-name": "off"
      }
    ],
    "axe/name-role-value": [
      "default",
      {
        "button-name": "off",
        "link-name": "off"
      }
    ],
    "typescript-config/strict": "off",
    "no-inline-styles": "off",
    "compat-api/css": "off",
    "axe/text-alternatives": [
      "default",
      {
        "image-alt": "off",
        "frame-title": "off"
      }
    ],
    "button-type": "off",
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "iframe[allow]"
        ]
      }
    ]
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_chr <= 119",
    "not chrome <= 119",
    "not edge <= 119",
    "not opera <= 104",
    "not samsung <= 23"
  ]
}